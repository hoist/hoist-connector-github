{
  "name": "GitHub",
  "slug": "github",
  "url": "http://www.github.com",
  "auth_types": {
    "Public": {
      "setup_help": "./samples/setupHelp.md",
      "requires_signing": true,
      "features": [
        "Access multiple GitHub Accounts",
        "Hoist handles the OAuth"
      ]
    }
  },
  "settings": [{
    "key": "clientId",
    "type": "text",
    "description": "Client Id",
    "for_types": ["Public"],
    "required": true
  }, {
    "key": "clientSecret",
    "type": "text",
    "description": "Client Secret",
    "for_types": ["Public"],
    "required": true
  }, {
    "key": "scope",
    "type": "text",
    "description": "Comma separated list of scopes see https://developer.github.com/v3/oauth/#scopes",
    "for_types": ["Public"],
    "required": true
  }],
  "samples": {
    "getting_started": "./samples/gettingStarted.js"
  },
  "endpoints": {}
}
